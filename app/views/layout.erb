  
<!doctype html>
<html>
  <head>
    <title>fwitter</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/style.css">
  </head>
  <body>
    
    <nav>
      <% if logged_in? %>
        <a href="/tweets/new"> create tweet </a>
