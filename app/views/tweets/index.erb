<ul>
  <% @tweets.each do |tweet| %>
    <li><%= tweet.content %> 
      <i>posted by <%= tweet.user.username %> 
      on <%= tweet.created_at.strftime("%-m/%-d %H:%M").downcase %>.</i>
      <% if logged_in? && tweet.user == current_user %>
        <a href="/tweets/<%= tweet.id %>/edit" >edit</a>. <a href="/tweets/<%= tweet.id %>/delete" >delete</a>.
      <% end %>
    </li>
  <% end %>
</ul>